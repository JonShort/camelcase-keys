{
  "name": "keys-to-camelcase",
  "version": "1.0.1",
  "description": "Convert object keys to camelCase",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JonShort/keys-to-camelcase"
  },
  "author": {
    "name": "Jon Short",
    "url": "https://jonshort.me"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "rimraf lib/ && rollup -c",
    "clean": "rimraf lib/",
    "test": "ava"
  },
  "files": [
    "lib/index.cjs.js",
    "lib/index.es.js",
    "lib/index.umd.js"
  ],
  "browser": "lib/index.umd.js",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "keywords": [
    "map",
    "obj",
    "object",
    "key",
    "keys",
    "value",
    "values",
    "val",
    "iterate",
    "camelcase",
    "camel-case",
    "camel",
    "case",
    "dash",
    "hyphen",
    "dot",
    "underscore",
    "separator",
    "string",
    "text",
    "convert",
    "deep",
    "recurse",
    "recursive",
    "browser"
  ],
  "dependencies": {
    "camelcase": "^4.1.0",
    "map-obj": "^2.0.0",
    "quick-lru": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "ava": "1.0.0-beta.8",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.65.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-terser": "^2.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{jsx,js,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
